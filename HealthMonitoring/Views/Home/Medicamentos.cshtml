@model Common.PersonModel

@{
    Layout = null;
    ViewData["Title"] = "Medicamentos";
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - HealthMonitoring</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link href="~/css/medicamentos.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Symbols+Rounded:opsz,wght,FILL,GRAD@40,400,0,0" />
    <script src="https://code.jquery.com/jquery-1.9.1.js"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
</head>
<body>
    <input type="checkbox" id="check">
    <!--header começo-->
    <header>
        <label for="check">
            <img src="~/iconv2.png" name="menu-outline" id="sidebar_btn" />
        </label>
    </header>
    <!--header final-->
    <!--sidebar começo-->
    <div class="sidebar">
        <center>
            <img src="~/tcclogo.png" class="image" alt="logo" />
        </center>
        <a href="/Home"><ion-icon name="home-outline"></ion-icon><span>Home</span></a>
        <a href="/Home/Usuario"><ion-icon name="people-outline"></ion-icon><span>Usuário</span></a>
        <a href="/Home/Medicamentos"><ion-icon name="medical-outline"></ion-icon><span>Medicamentos</span></a>
        <a href="/Home/Extras"><ion-icon name="extension-puzzle-outline"></ion-icon><span>Extras</span></a>
        <a href="/Login"><ion-icon name="log-out-outline"></ion-icon><span>Sair</span></a>
    </div>
    <div class="body-card">
        <div class="card-button">
            <a href="#" class="btn btn-primary">Adicionar</a>
        </div>
        <br />
        <div class="card" style="width: 20rem;">
            <div class="card-body">
                <h5 class="card-title">Medicamento</h5>
                <h6 class="card-subtitle mb-2 text-muted">Dipirona</h6>
                <div class="progress">
                    <div class="progress-bar progress-bar-striped" role="progressbar" aria-label="Info striped example" style="width: 10%" aria-valuenow="10" aria-valuemin="0" aria-valuemax="100"></div>
                </div>
                <br />
                <h6 class="card-subtitle mb-2 text-muted">Último registro: 26/10/2022 08:00</h6>
                <a href="#" class="btn btn-primary">Editar</a>
                <a href="#" class="btn btn-primary">Excluir</a>
            </div>
        </div>
        <br />
        <div class="card" style="width: 20rem;">
            <div class="card-body">
                <h5 class="card-title">Medicamento</h5>
                <h6 class="card-subtitle mb-2 text-muted">Paracetamol</h6>
                <div class="progress">
                    <div class="progress-bar progress-bar-striped" role="progressbar" aria-label="Info striped example" style="width: 25%" aria-valuenow="10" aria-valuemin="0" aria-valuemax="100"></div>
                </div>
                <br />
                <h6 class="card-subtitle mb-2 text-muted">Data: 26/10/2022 09:00</h6>
                <a href="#" class="btn btn-primary">Editar</a>
                <a href="#" class="btn btn-primary">Excluir</a>
            </div>
        </div>
        <br />
        <div class="card" style="width: 20rem;">
            <div class="card-body">
                <h5 class="card-title">Medicamento</h5>
                <h6 class="card-subtitle mb-2 text-muted">Comprimido</h6>
                <div class="progress">
                    <div class="progress-bar progress-bar-striped" role="progressbar" aria-label="Success striped example" style="width: 100%" aria-valuenow="10" aria-valuemin="0" aria-valuemax="100"></div>
                </div>
                <br />
                <h6 class="card-subtitle mb-2 text-muted">Data: 26/10/2022 12:00</h6>
                <a href="#" class="btn btn-primary">Editar</a>
                <a href="#" class="btn btn-primary">Excluir</a>
            </div>
        </div>
    </div>
    <!--sidebar final-->
    <script type="module" src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.esm.js"></script>
    <script nomodule src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.js"></script>
</body>
</html>